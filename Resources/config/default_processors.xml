<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="rz_search.field_processor.date.class">Rz\SearchBundle\FieldProcessor\DateFieldProcessor</parameter>
    </parameters>

    <services>
        <service id="rz_search.field_processor.date" class="%rz_search.field_processor.date.class%">
            <tag name="rz_search.field_processor"/>
            <argument>%rz_search.field_processor.date.date_format%</argument>
        </service>
    </services>
</container>
